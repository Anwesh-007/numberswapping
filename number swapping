#include <stdio.h>
int main()
{
    printf ("Enter your age: ");
    int age= 0;
    scanf("%d", &age);
    printf ("You said that your age is: %d\n", age);
    return 0;
}

#include <stdio.h>

int main() {
    printf ("Enter your name: ");
    char name [10];
    scanf("%s", &name);
    printf("You said that your name is: %s", name);
    return 0;
}
